package Classes;

public class Avaliacao {
	private int id;
	private boolean gostei;
	private boolean naoGostei;
	private int candidaturaId;
	private int usuarioId;
	
	
	public Avaliacao(int id, boolean gostei, boolean naoGostei,  int candidaturaId, int usuarioId) {
		this.id = id;
		this.gostei = gostei;
		this.naoGostei = naoGostei;
		this.candidaturaId = candidaturaId;
		this.candidaturaId = usuarioId;
	}

	
}
