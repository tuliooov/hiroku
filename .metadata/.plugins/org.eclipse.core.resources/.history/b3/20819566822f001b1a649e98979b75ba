package Classes;

public class Avaliacoes {
	private String cpf;
	private int likes;
	private int deslikes;
	private int candidaturaId;
	
	public Avaliacoes(String cpf, int likes, int deslikes, int candidaturaId) {
		this.cpf = cpf;
		this.likes = likes;
		this.deslikes = deslikes;
		this.candidaturaId = candidaturaId;
	}

}
